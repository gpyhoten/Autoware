<launch>
  <!-- node parameters -->
  <arg name="map_topic" default="/realtime_cost_map" />

  <!-- Launch the grid map simple demo node -->
  <node pkg="object_map" type="vmap_og" name="vmap_og" output="screen">
    <param name="map_topic" value="$(arg map_topic)" />
  </node>


<!-- Launch the grid map visualizer -->
  <node pkg="grid_map_visualization" type="grid_map_visualization" name="grid_map_visualization" output="screen">
    <rosparam command="load" file="$(find object_map)/config/vmap_og.yaml" />
  </node>

</launch>
